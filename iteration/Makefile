run-c:
	@echo "Running C program..."
	gcc main.c -o main && ./main
run-rust:
	@echo "Running Rust program..."
	rustc main.rs && ./main
run-java:
	@echo "Running Java program..."
	javac Main.java && java Main
run-python:
	@echo "Running Python program..."
	python3 main.py
run-node:
	@echo "Running Node.js program..."
	node main.js
run-go:
	@echo "Running Go program..."
	go run main.go
run-all: run-c run-rust run-java run-python run-node run-go
	@echo "All programs executed successfully."